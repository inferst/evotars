{
  "version": "0.0.6",
  "name": "evotars",
  "description": "Show animated characters on stream",
  "author": "Mikhail Morozov",
  "url": "https://github.com/inferst/evotars",
  "repository": {
    "type": "git",
    "url": "https://github.com/inferst/evotars"
  },
  "keywords": [
    "stream",
    "avatars",
    "chatters"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "tsc --noEmit"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "source": "./src/index.ts",
  "main": "./dist/evotars.umd.cjs",
  "module": "./dist/evotars.js",
  "exports": {
    ".": {
      "import": "./dist/evotars.js",
      "require": "./dist/evotars.umd.cjs"
    }
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/tinycolor2": "^1.4.6",
    "typescript": "^5.0.2",
    "vite": "^5.2.9",
    "vite-plugin-dts": "^3.8.3"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^23.1.1",
    "pixi.js": "^7.4.2",
    "tinycolor2": "^1.6.0"
  }
}
